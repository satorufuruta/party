{
  "name": "party",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "cf:worker:dev": "wrangler dev --local",
    "cf:worker:deploy": "wrangler deploy",
    "cf:pages:dev": "wrangler pages dev",
    "cf:pages:build": "npx @cloudflare/next-on-pages",
    "cf:pages:deploy": "wrangler pages deploy .vercel/output/static",
    "cf:d1:migrate": "wrangler d1 migrations apply party-db --remote",
    "cf:d1:migrate:dev": "wrangler d1 migrations apply party-db --local",
    "cf:d1:migration:create": "wrangler d1 migrations create --name"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251003.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.42.1"
  }
}
